<%- include layouts/headers.ejs %>
    <div class="container-fluid"><br/>
      <h2 class="text-white mb-2"><%= title %></h2>
    
      <div class="row class" style="width: 80rem col-sm-13 ">

        <% for(var ejemplar=0;ejemplar<Ejemplares.length;ejemplar++) { %>
